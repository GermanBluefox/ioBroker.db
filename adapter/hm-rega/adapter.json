{
    "common": {
        "name":                     "hm-rega",
        "version":                  "0.0.1",
        "mode":                     "daemon",
        "language":                 "javascript",
        "loglevel":                 "info"
    },
    "native": {
        "ip":                       "172.16.23.3",
        "port":                     8181,
        "polling":                  true,
        "pollingInterval":          30,
        "pollingTrigger":           "BidCoS-RF:50.PRESS_SHORT",
        "rfdAdapter":               "hm-rpc.0",
        "hs485dAdapter":            null,
        "cuxdAdapter":              null
    },
    "objects": [
        {
            "_id": "_design/hm-rega",
            "language": "javascript",
            "views": {
                "variables": {
                    "map": "function(doc) {\n  if (doc._id.match(/^hm-rega\\.[0-9]+\\.[0-9]+/) && (doc.native.TypeName === 'ALARMDP' || doc.native.TypeName === 'VARDP')) {\n   emit(doc._id, doc);\n  }\n}"
                }

            }
        }
    ]
}